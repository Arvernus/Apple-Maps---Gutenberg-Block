{
	"title": "Mapkit",
	"description": "Apple Maps",
	"category": "common",
	"icon": {
		"src": "location-alt"
	},
	"supports": {
		"align": ["wide", "full"]
	},
	"attributes": {
		"mapType": {
			"type": "string",
			"default": "standard"
		},
		"searchQuery": {
			"type": "string"
		},
		"showsMapTypeControl": {
			"type": "Boolean",
			"default": true
		},
		"showsZoomControl": {
			"type": "Boolean",
			"default": true
		},
		"pointLatitude": {
			"type": "number",
			"default": 51.5237503
		},
		"pointLongitude": {
			"type": "number",
			"default": -0.158555
		},
		"pointTitle": {
			"type": "string",
			"default": "221B Baker Street"
		},
		"pointSubtitle": {
			"type": "string",
			"default": "London"
		},
		"pointGlyphText": {
			"type": "string",
			"default": ""
		},
		"pointColor": {
			"type": "string",
			"default": "#000000"
		},
		"authenticated": {
			"type": "boolean",
			"default": false
		}
	}
}
